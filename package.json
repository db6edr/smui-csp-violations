{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "node ./scripts/express.js",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "npm run smui-theme-light",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/theme"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-typescript": "^8.2.5",
    "@tsconfig/svelte": "^2.0.1",
    "express": "^4.17.1",
    "rollup": "^2.58.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.43.1",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.8",
    "tslib": "^2.3.1",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@material/elevation": "^12.0.0",
    "@material/theme": "^12.0.0",
    "@material/typography": "^12.0.0",
    "rollup-plugin-delete": "^2.0.0",
    "smui-theme": "^4.2.0",
    "svelte-material-ui": "^4.2.0"
  }
}
